script/source = "extends KinematicBody2D

var run_speed = 800
var jump_speed = -1200
var gravity = 2500

var velocity = Vector2()

var cursor_image = load(\"res://assets/png/cross_new.png\")
var max_hp = 100
var current_hp

var in_tunnel = false

func _ready() -> void:
	cursor()
	current_hp = max_hp

func cursor():
	Input.set_custom_mouse_cursor(cursor_image,
			Input.CURSOR_ARROW,
			Vector2(14, 14))

func get_input():
	velocity.x = 0
	var right = Input.is_action_pressed(\"move_right\")
	var left = Input.is_action_pressed('move_left')
	var jump = Input.is_action_pressed('jump')
	var crouch = Input.is_action_just_pressed('crouch')
	
	if is_on_floor() and jump:
		velocity.y = jump_speed
		get_node(\"AnimatedSprite\").play(\"jump\")
	if right:
		velocity.x += run_speed
		$AnimatedSprite.flip_h = false
		$AnimatedSprite.position.x = 40
		$tunnel_detector.rotation_degrees = 0
		$wall_detector.rotation_degrees = 0
		$tunnel_detector.position.x = 30
		$wall_detector.position.x = 30
		if in_tunnel == true:
			get_node(\"AnimatedSprite\").play(\"crouch\")
		else:
			get_node(\"AnimatedSprite\").play(\"run\")
	elif left:
		velocity.x -= run_speed
		$AnimatedSprite.flip_h = true
		$AnimatedSprite.position.x = -45
		$tunnel_detector.rotation_degrees = 180
		$wall_detector.rotation_degrees = 180
		$tunnel_detector.position.x = -30
		$wall_detector.position.x = -30
		if in_tunnel == true:
			get_node(\"AnimatedSprite\").play(\"crouch\")
		else:
			get_node(\"AnimatedSprite\").play(\"run\")

	if velocity.x == 0:
		if in_tunnel == true:
			get_node(\"AnimatedSprite\").play(\"crouch\")
		else:
			get_node(\"AnimatedSprite\").play(\"idle\")
		
	if $wall_detector.is_colliding() and !$tunnel_detector.is_colliding():
		in_tunnel = true
		$CollisionShape2D.shape.extents = Vector2(40,70)
		$CollisionShape2D.position.y = -28
		$CollisionShape2D.position.x = 50
	else:
		in_tunnel = false
		$CollisionShape2D.shape.extents = Vector2(64,50)
		$CollisionShape2D.position.y = -49
		$CollisionShape2D.position.x = 0
		
func _physics_process(delta):
	velocity.y += gravity * delta
	get_input()
	velocity = move_and_slide(velocity, Vector2(0, -1))
	player_damage()
	print($CollisionShape2D.shape.extents)

func player_damage():
	var damaged: = Input.is_action_just_pressed(\"fire\")
	if damaged:
		current_hp -= 10
		print(\"PLAYER HIT\")
		get_node(\"PlayerHealth\").value = int((float(current_hp) / max_hp) * 100)
